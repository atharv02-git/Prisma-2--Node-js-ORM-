generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model test {
  id     Int      @id
  active Boolean?
}

model user {
  id       Int    @id @default(autoincrement())
  username String @unique(map: "User_username_key") @db.VarChar(255)
}
